{
	"name": "swagger2struct",
	"displayName": "swagger2struct",
	"description": "fetch swagger json and convert to struct.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.configureApiURLPath",
		"onCommand:extension.configureApiJsonFile",
		"onCommand:extension.configureEnvFile",
		"onCommand:extension.generate",
		"onCommand:extension.merge",
		"onCommand:extension.sync",
		"onCommand:extension.help",
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"category": "Swagger to struct",
				"command": "extension.configureApiURLPath",
				"title": "configure API doc.json URL path(relative `BASE_URL` in dev.env.js)"
			},
			{
				"category": "Swagger to struct",
				"command": "extension.configureApiJsonFile",
				"title": "configure API doc.json file"
			},
			{
				"category": "Swagger to struct",
				"command": "extension.configureEnvFile",
				"title": "configure current env file"
			},
			{
				"category": "Swagger to struct",
				"command": "extension.generate",
				"title": "generate by schema"
			},
			{
				"category": "Swagger to struct",
				"command": "extension.merge",
				"title": "merge current struct with schema"
			},
			{
				"category": "Swagger to struct",
				"command": "extension.help",
				"title": "Open help document"
			},
			{
				"category": "Swagger to struct",
				"command": "extension.sync",
				"title": "Sync API doc.json"
			},
			{
				"category": "Swagger to struct",
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/fs-extra": "^8.0.1",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"cz-conventional-changelog": "^3.0.2",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"fs-extra": "^8.1.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
